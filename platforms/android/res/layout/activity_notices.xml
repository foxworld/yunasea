<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context=".NoticesActivity"
    >
    <WebView
        android:id="@+id/wv_notices"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
    <ProgressBar android:id="@+id/progress_bar_notices"
                 style="?android:attr/progressBarStyleHorizontal"
                 android:layout_width="fill_parent" android:layout_height="2dp"
                 android:layout_gravity="top" android:max="100"  android:progressDrawable="@drawable/progressbar_custom"
        />
</RelativeLayout>
